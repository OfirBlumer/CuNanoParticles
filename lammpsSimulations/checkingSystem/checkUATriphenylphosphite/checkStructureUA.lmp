# LAMMPS Input file generated by Avogadro
# Title

# Initialization
units          real
#dimension      3d
boundary       p p p
atom_style     full
pair_style lj/cut 10
bond_style harmonic
angle_style harmonic
dihedral_style fourier
improper_style cvff

read_data      UAtriphenylphosphite.lmps
# Groups Definition

 group bond1	id 1 16 2 9
 group bond2	id 3 4 8 10 15 11 17 18 22
 group bond3	id 3 2 9 10 11 16
 group bond5	id 4 5 6 7 8 11 12 13 14 15 18 19 20 21 22

 group angle1   id 3 4 8 10 11 15 17 18 22
 group angle2   id 4 5 6 7 8 11 12 13 14 15 18 19 20 21 22
 group angle4   id 1 2 16 9
 group angle6   id 3 4 5 7 8 10 11 12 14 15 17 18 19 21 22
# Settings
dump                    mydmp all atom 5000 checkStructureDump.lammpstrj

velocity       all create 298.25 4928459 rot yes mom yes dist gaussian
fix            ensemble all npt temp 298.25 298.25 10 iso 1 1 100.0
compute  1 bond1 bond/local dist
compute  2 bond2 bond/local dist
compute  3 bond3 bond/local dist
compute  5 bond5 bond/local dist
compute  6 angle1 angle/local theta
compute  7 angle2 angle/local theta
compute  9 angle4 angle/local theta
compute  11 angle6 angle/local theta

dump 1  bond1    local 1000 bonds/bond1.dump  c_1
dump 2  bond2    local 1000 bonds/bond2.dump  c_2
dump 3  bond3    local 1000 bonds/bond3.dump  c_3
dump 5  bond5    local 1000 bonds/bond4.dump  c_5
dump 6  angle1    local 1000 angles/angle1.dump  c_6
dump 7  angle2    local 1000 angles/angle2.dump  c_7
dump 9  angle4    local 1000 angles/angle3.dump  c_9
dump 11  angle6    local 1000 angles/angle4.dump  c_11


timestep            0.1

# Output
thermo_style custom step temp etotal press vol density
thermo         100000

# Run the simulation
run            1000000
